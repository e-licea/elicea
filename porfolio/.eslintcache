eturn (\n        <div id = 'Contact'>\n            <h3 className=\"Main-h3\">Contact</h3>\n            <form onSubmit = {onSubmit} >\n                <label>Name :\n                    <input type=\"text\"\n                        onChange = {onChange}\n                        name = \"name\"\n                        value = {formData.name}\n                        placeholder ='Enter your name'\n                    />\n                </label>\n                <label htmlFor=\"\">Email :\n                    <input type=\"text\"\n                    onChange = {onChange}\n                    name = 'email'\n                    value = {formData.email}\n                    placeholder ='youemail@email.com'\n                    />\n                </label>\n                <label htmlFor=\"\">Subject :\n                    <input type=\"text\"\n                    onChange = {onChange}\n                    name = 'subject'\n                    value = {formData.subject}\n                    placeholder = 'Re: Subject'\n                    />\n                </label>\n                <label htmlFor=\"\">Message :\n                    <textarea type=\"text\"\n                    onChange = {onChange}\n                    name = 'message'\n                    value = {formData.message}\n                    placeholder = 'Your message here'\n                    />\n                </label>\n                <button>Submit</button>\n            </form>\n        </div>\n    )\n}","/home/xubuntu/Documents/Projects/elicea/porfolio/src/Components/Section/Skills.jsx",[],"/home/xubuntu/Documents/Projects/elicea/porfolio/src/Components/Section/About.jsx",[],"/home/xubuntu/Documents/Projects/elicea/porfolio/src/Components/Section/SlideShow.jsx",["70","71","72","73"],"import React, { Component } from 'react'\nimport { useSpring, animated } from 'react-spring'\n\n\n\nexport default class SlideShow extends Component {\n\n\n  constructor(props){\n    super(props)\n    this.state = {\n      active : imgArr[0],\n      imgArr: imgArr\n    }\n    this.onClick = this.onClick.bind(this);\n  }\n\n\n  componentWillMount(){\n    console.log('The component will now mount')\n\n  }\n  \n  componentDidMount(){\n    console.log(this.state)\n    setInterval(()=>{\n      this.setState({active: this.state.active.id == 3? imgArr[0] : imgArr[this.state.active.id + 1]})\n    }, 10000)\n  }\n\n\n\n  onClick(e){\n    e.preventDefault();\n    if (e.target.id != 'next'){\n      console.log('prev')\n      this.setState({\n        active: this.state.active.id == 0? imgArr[3] : imgArr[this.state.active.id - 1],\n      })\n    }\n    else {\n      console.log('next')\n      this.setState({\n        active: this.state.active.id == 3? imgArr[0] : imgArr[this.state.active.id + 1],\n      })\n    }\n    \n  }\n\n\n  render() {\n    return (\n      <div id = 'SlideShow'>\n        <div className=\"photo-container\">\n          <div \n            id = 'prev'\n            onClick={this.onClick}\n            ></div>\n              <ActiveImg active = {this.state.active}/>\n\n          <div \n            id = 'next'\n            onClick={this.onClick}\n            ></div>\n\n        </div>\n      </div>\n    )\n  }\n}\n\n\nfunction ActiveImg(props){\n\n  let { active } = props\n  const styles = useSpring({\n    config: {delay: 10000, duration: 400},\n    to: async (next, cancel) => {\n      await next({ opacity: '0', marginTop: '800px' })\n      await next({ opacity: '1', marginTop: '0px' })\n\n    },\n    from:{ opacity: '0', marginTop: '800px' }\n  })\n\n  return(\n      <animated.div  style= {styles}>\n        <img id = {active.id} src={active.imgSrc} alt=\"\" />\n      </animated.div>\n  )\n}\n\n\n\nconst imgArr = [\n  {\n    id: 0,\n    imgSrc: 'https://images.ctfassets.net/hrltx12pl8hq/3MbF54EhWUhsXunc5Keueb/60774fbbff86e6bf6776f1e17a8016b4/04-nature_721703848.jpg?fit=fill&w=480&h=270',\n  },  {\n    id: 1,\n    imgSrc: 'https://media.istockphoto.com/photos/the-african-king-picture-id492611032?k=6&m=492611032&s=612x612&w=0&h=Y_CHJCbkFOqmXvHOYSdxu0T5UZqoRj7OzpIBLGkvf_Q=',\n  },  {\n    id: 2,\n    imgSrc: 'https://cdn.eso.org/images/thumb300y/eso1322a.jpg',\n  },  {\n    id: 3,\n    imgSrc: 'https://spaceplace.nasa.gov/gallery-sun/en/solar-flare.en.jpg',\n  }\n]","/home/xubuntu/Documents/Projects/elicea/porfolio/src/Components/Hooks/UseWindowSize.jsx",[],{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","severity":1,"message":"79","line":19,"column":25,"nodeType":"80","endLine":19,"endColumn":56},{"ruleId":"78","severity":1,"message":"79","line":20,"column":25,"nodeType":"80","endLine":20,"endColumn":56},{"ruleId":"81","severity":1,"message":"82","line":9,"column":13,"nodeType":"83","messageId":"84","endLine":9,"endColumn":23},{"ruleId":"85","severity":1,"message":"86","line":12,"column":15,"nodeType":"87","endLine":15,"endColumn":6},{"ruleId":"81","severity":1,"message":"88","line":9,"column":8,"nodeType":"83","messageId":"84","endLine":9,"endColumn":13},{"ruleId":"81","severity":1,"message":"89","line":1,"column":26,"nodeType":"83","messageId":"84","endLine":1,"endColumn":35},{"ruleId":"90","severity":1,"message":"91","line":27,"column":51,"nodeType":"92","messageId":"93","endLine":27,"endColumn":53},{"ruleId":"90","severity":1,"message":"94","line":35,"column":21,"nodeType":"92","messageId":"93","endLine":35,"endColumn":23},{"ruleId":"90","severity":1,"message":"91","line":38,"column":38,"nodeType":"92","messageId":"93","endLine":38,"endColumn":40},{"ruleId":"90","severity":1,"message":"91","line":44,"column":38,"nodeType":"92","messageId":"93","endLine":44,"endColumn":40},"no-native-reassign",["95"],"no-negated-in-lhs",["96"],"jsx-a11y/anchor-is-valid","The href attribute requires a valid value to be accessible. Provide a valid, navigable address as the href value. If you cannot provide a valid href, but still need the element to resemble a link, use a button and change it with appropriate styles. Lea